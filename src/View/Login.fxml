<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="382.0" prefWidth="360.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.LoginController">
   <children>
      <GridPane layoutX="43.0" layoutY="34.0" prefHeight="304.0" prefWidth="291.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="257.0" minWidth="10.0" prefWidth="174.0" />
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="290.0" minWidth="10.0" prefWidth="228.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Label fx:id="loginLabel" alignment="CENTER" prefHeight="122.0" prefWidth="281.0" text="Login">
               <font>
                  <Font name="System Bold" size="18.0" />
               </font>
            </Label>
            <Label fx:id="usernameLabel" prefHeight="17.0" prefWidth="120.0" text="Username" GridPane.rowIndex="1">
               <font>
                  <Font size="14.0" />
               </font></Label>
            <TextField fx:id="loginUsername" prefHeight="25.0" prefWidth="222.0" GridPane.columnIndex="1" GridPane.rowIndex="1" />
            <Label fx:id="passwordLabel" text="Password" GridPane.rowIndex="2">
               <font>
                  <Font size="14.0" />
               </font></Label>
            <TextField fx:id="loginPassword" GridPane.columnIndex="1" GridPane.rowIndex="2" />
            <Label fx:id="locationLabel" text="Location" GridPane.rowIndex="3">
               <font>
                  <Font size="14.0" />
               </font></Label>
            <TextField fx:id="loginLocation" editable="false" GridPane.columnIndex="1" GridPane.rowIndex="3" />
            <Button fx:id="loginButton" mnemonicParsing="false" onAction="#loginButtonAction" text="Login" GridPane.rowIndex="4" GridPane.valignment="CENTER" />
            <Button fx:id="exitButton" mnemonicParsing="false" onAction="#exitButtonAction" text="Exit" GridPane.columnIndex="1" GridPane.rowIndex="4" />
         </children>
      </GridPane>
   </children>
</AnchorPane>
